/* Rose Pine Moon - Rofi colors */
/* Generated from theme.yml */
/* Author: Ghostty */

* {
    /* Base16 palette */
    base00: #232136;  /* Main background */
    base01: #6e6a86;  /* Lighter background */
    base02: #44415a;  /* Selection background */
    base03: #6e6a86;  /* Comments, muted */
    base04: #e0def4;  /* Dark foreground */
    base05: #e0def4;  /* Default foreground */
    base06: #e0def4;  /* Light foreground */
    base07: #e0def4;  /* Brightest foreground */
    base08: #eb6f92;  /* Red */
    base09: #eb6f92;  /* Orange */
    base0A: #f6c177;  /* Yellow */
    base0B: #3e8fb0;  /* Green */
    base0C: #ea9a97;  /* Cyan */
    base0D: #9ccfd8;  /* Blue */
    base0E: #c4a7e7;  /* Magenta */
    base0F: #c4a7e7;  /* Brown */

    /* Semantic colors */
    bg: #232136;
    bg-alt: #6e6a86;
    bg-selected: #44415a;
    fg: #e0def4;
    fg-alt: #e0def4;
    fg-muted: #6e6a86;
    fg-bright: #e0def4;
    border: #44415a;

    /* Status colors - use extended diagnostic colors when available */

    active: #3e8fb0;
    selected: #9ccfd8;

    /* Named accent colors */
    red: #eb6f92;
    green: #3e8fb0;
    yellow: #f6c177;
    blue: #9ccfd8;
    magenta: #c4a7e7;
    cyan: #ea9a97;
    orange: #eb6f92;

    /* Bright variants */
    bright-red: #eb6f92;
    bright-green: #3e8fb0;
    bright-yellow: #f6c177;
    bright-blue: #9ccfd8;
    bright-magenta: #c4a7e7;
    bright-cyan: #ea9a97;
}

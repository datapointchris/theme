    active: #519f50;
    selected: #2c78bf;

    /* Named accent colors */
    red: #ef2f27;
    green: #519f50;
    yellow: #fbb829;
    blue: #2c78bf;
    magenta: #e02c6d;
    cyan: #0aaeb3;
    orange: #f75341;

    /* Bright variants */
    bright-red: #f75341;
    bright-green: #98bc37;
    bright-yellow: #fed06e;
    bright-blue: #68a8e4;
    bright-magenta: #ff5c8f;
    bright-cyan: #2be4d0;
}

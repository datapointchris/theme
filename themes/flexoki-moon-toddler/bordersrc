#!/bin/bash
# Flexoki Moon Toddler - JankyBorders theme
# Generated from theme.yml
# Author: datapointchris

options=(
  style=round
  width=8.0
  hidpi=on

  # Active window - use UI accent color
  active_color=0xff4f7edc

  # Inactive windows - use UI border color
  inactive_color=0xff726250

  # Glow effect using accent color
  active_color='glow(0xff4f7edc)'

  blacklist="System Settings|System Information|QuickTime Player"
)

borders "${options[@]}"
